<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <servlet>
    <servlet-name>initDataAction</servlet-name>
    <servlet-class>com.lijun.action.initDataAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>showUserDataAction</servlet-name>
    <servlet-class>com.lijun.action.showUserDataAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>com.lijun.action.test</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>initDataAction</servlet-name>
    <url-pattern>/initDataAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>showUserDataAction</servlet-name>
    <url-pattern>/showUserDataAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/servlet/test</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>